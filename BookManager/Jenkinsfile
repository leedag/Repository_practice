pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                // 빌드 관련 스크립트를 여기에 추가
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                // 테스트 관련 스크립트를 여기에 추가
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                // 배포 관련 스크립트를 여기에 추가
            }
        }
    }
}
